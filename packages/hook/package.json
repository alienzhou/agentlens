{
  "name": "@agentlens/hook",
  "version": "0.1.0",
  "description": "AgentLens hook system - Agent adapters, event capture, and session monitoring",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@agentlens/core": "workspace:*",
    "chokidar": "^4.0.1"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "license": "MIT"
}
