{
  "name": "@vibe-x/agent-blame-vscode",
  "version": "0.1.0",
  "displayName": "Agent Blame",
  "description": "VS Code extension for Agent Blame - Track AI agent contributions",
  "publisher": "alienzhou",
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "contributes": {
    "commands": [
      {
        "command": "agent-blame.showBlame",
        "title": "Agent Blame: Show Blame"
      },
      {
        "command": "agent-blame.connectAgent",
        "title": "Agent Blame: Connect Agent"
      },
      {
        "command": "agent-blame.disconnectAgent",
        "title": "Agent Blame: Disconnect Agent"
      },
      {
        "command": "agent-blame.showStatus",
        "title": "Agent Blame: Show Agent Status"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agent-blame",
          "title": "Agent Blame",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "agent-blame": [
        {
          "id": "agent-blame.agents",
          "name": "Connected Agents"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@agent-blame/core": "workspace:*",
    "@vibe-x/agent-blame": "workspace:*"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "license": "MIT"
}
